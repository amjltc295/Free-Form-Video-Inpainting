{
    "name": "clouds_test",
    "trainer": {
        "epochs": 1000,
        "save_dir": "saved/",
        "save_freq": 5,
        "verbosity": 2,
        "monitor": "test0_p_dist",
        "monitor_mode": "min",
        "mask_only": true,
        "pretrained_load_strict": false,
        "evaluate_score": true
    },
    "inference_only": true,
    "test_data_loader": [
        {
            "type": "MaskedFrameDataLoader",
            "args":{
                "name": "test_cloud_removal",
                "root_videos_dir": "../dataset/clouds/inpaint/test/image/",
                "root_masks_dir": "../dataset/clouds/inpaint/test/mask/",
                "root_outputs_dir": "../VOS_resized2",
                "dataset_args": {
                    "type": "video",
                    "w": 64,
                    "h": 64,
                    "sample_length": 5,
                    "random_sample": false,
                    "random_sample_mask": false,
                    "mask_type": "as_video",
                    "mask_dilation": 0
                },
                "batch_size": 1,
                "shuffle": false,
                "validation_split": 0.0,
                "num_workers": 4
            }
        }
    ]
}
